@charset "utf-8";
/* CSS Document */

#my{
	background:none;
	height:90px;
	padding:25px;
	padding-left:50px;
	color:#000;
	position:absolute;
	margin-left:-50px;
	margin-top:-20px;
}
.mydash{
	background:rgba(51,51,51,0.9);
	width:250%;
	display:none;
	margin-top:-40px;
	height:670px;
	text-align:center;
	position:absolute;
	z-index:1;
	margin-left:-68%;
	}
.tx1{
	font-size:28px;
	color:#CCC;
	float:right;
	display:block;
	cursor:pointer;
	position:absolute;
	margin-left:87%;
	margin-top:30px;
	font-weight:bolder;
	z-index:100;
	}
	
#camera{
	float:left;
	}	
	
.photos{
	margin-left:10px;
	font-size:15px;
	float:left;
	color:#930;
	}	
	
.tag{
	margin-left:-50px;
	font-size:13px;
	color:#930;
	width:30%;
	float:right
	background:rgba(204,204,204,0.2);
	padding:2px;
	padding-left:10px;
	padding-right:10px;
	border:rgba(102,102,102,1) solid 1px;
	cursor:pointer;
	}	
.pr{
	float:left;
	height:20px;
	width:20px;
	margin-top:20px;
	}
.privacy{
	margin-right:330px;
	margin-top:20px;
	color:#930;
	background-color:rgba(204,204,204,0.2);
	}
.btn{
	color:#930;
	margin-top:-20px;
	padding:0px;
	font-size:14px;
	padding-left:30px;
	padding-right:40px;
	background:rgba(204,204,204,0.2);
	border:rgba(102,102,102,1) solid 1px;
	}

.post{
	background:rgba(153,51,0,1);
	color:#FFF;
	width:100%;
	text-align:center;
	padding:10px;
	margin-top:20px;
	border-radius:10px;
	cursor:pointer;
	}	
	
.em{
	z-index:100;
	}

.im{
	float:right;
	height:20px;
	width:20px;
	position:absolute;
	z-index:1;
	margin-top:8px;
	margin-left:89%;
	}
.e1{
	height:200px;
	width:100%;
	background:rgba(204,204,204,0.5);
	padding-top:30px;
	display:none;
	}	
	
	
	
	
	
	
	
	
	
	
	
	
	